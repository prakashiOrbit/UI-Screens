{
  "division": {
    "name": "PO Header",
    "formelements": [
      {
        "id": "POID",
        "label": "PO ID",
        "control": "textbox",
        "validation": "alphanumeric",
        "disabled": true
      },
      {
        "id": "origin",
        "label": "Origin Location",
        "control": "select",
        "populateURL": "/Service/origin.json"
      },
      {
        "id": "date",
        "label": "Expected Date",
        "control": "date",
        "validate": "datetime"
      }
    ],
    "buttons": [
      {
        "id": "search",
        "label": "Search",
        "control": "button",
        "validationjs": "validatedata",
        "submitURL": "https://localhost/createPO"
      },
      {
        "id": "create",
        "label": "Create",
        "control": "button",
        "validationjs": "validatedata",
        "submitURL": "https://localhost/createPO"
      }
    ]
  }
}
